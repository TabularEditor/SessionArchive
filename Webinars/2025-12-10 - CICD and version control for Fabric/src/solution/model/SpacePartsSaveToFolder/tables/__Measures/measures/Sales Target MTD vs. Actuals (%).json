{
  "name": "Sales Target MTD vs. Actuals (%)",
  "expression": [
    "",
    "VAR _TARGET = [Sales Target MTD]",
    "VAR _ACTUAL = [Actuals MTD]",
    "VAR _DELTA  = _ACTUAL - _TARGET",
    "VAR _PERC   = DIVIDE ( _DELTA, _TARGET )",
    "",
    "RETURN",
    "",
    "_PERC"
  ],
