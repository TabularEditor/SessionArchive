{
  "name": "Orders Target vs. Net Orders (%)",
  "description": "Net orders exclude cancellation document types",
  "expression": [
    "",
    "VAR _TARGET = [Orders Target]",
    "VAR _DELTA  = [Net Orders] - _TARGET",
    "VAR _PERC   = DIVIDE ( _DELTA, _TARGET )",
    "RETURN",
    "_PERC"
